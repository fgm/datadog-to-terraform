{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2020"
    ],
    "module": "commonjs",
    "outDir": "dist",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2020"
  },
  "display": "Node 14",
  "exclude": ["__tests__", "node_modules"],
  "extends": "@tsconfig/node14/tsconfig.json",
  "include": ["app/**/*"],
}
